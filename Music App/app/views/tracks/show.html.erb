<h1> <%= @track.name %> </h1>

<%= button_to "Edit", edit_track_url(@track), method: :get %>


<p> <%= @track.style %> TRACK </p>

<p> Lyrics </p>

<p><%= @track.lyrics %></p>



<%= button_to "DELETE TRACK!?", track_url(@track), method: :delete %>
<%= button_to "Back", album_url(@track.album_id), method: :get %>

<p>Comments</p>
<br>
<% @track.notes.each do |note| %>
  <%= note.user.email %> says...
  <br>
  <%= note.note %>
  <br>
  <br>
<% end %>
<br>
<%= render "notes/form", track_id: params[:id] %>
